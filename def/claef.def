# 4.11.1
suite claef
  edit ECF_EXTN '.ecf'
  edit ACCOUNT 'atlaef'
  edit ECF_INCLUDE '/home/ms/at/kmcw/ecf/include'
  edit ECF_JOB_CMD '/usr/local/apps/schedule/1.4/bin/schedule kmcw cca %ECF_JOB% %ECF_JOBOUT%'
  edit ECF_MICRO '%'
  edit ECF_LOGHOST 'cca'
  edit ECF_HOME '/home/ms/at/kmcw/ecf'
  edit ECF_LOGPORT '36652'
  edit ECF_OUT '/scratch/ms/at/kmcw/ECF'
  edit CNF_DEBUG '0'
  family RUN_00
    edit VORHI '0'
    edit LAUF '00'
    edit DATUM '20190221'
    edit LEAD '6'
    family lbc
      edit GL '0'
      family MEM_12
        task getlbc
          trigger :GL == 0
          edit NAME 'getlbc12'
          edit KOPPLUNG '6'
          edit MEMBER '12'
          edit WALLT '01'
          edit NP '1'
          edit CLASS 'ns'
          label run ""
          label status ""
          label error "ok"
        task 901
          trigger :GL == 0 and getlbc == complete
          edit NAME '901_12'
          edit KOPPLUNG '6'
          edit MEMBER '12'
          edit WALLT '03'
          edit NP '1'
          edit CLASS 'ns'
          label run ""
          label status ""
          label error "ok"
        task getlbc_gl
          trigger :GL == 1
          edit NAME 'getlbcgl12'
          edit KOPPLUNG '6'
          edit MEMBER '12'
          edit WALLT '01'
          edit NP '1'
          edit CLASS 'ns'
          label run ""
          label status ""
          label error "ok"
        task gl
          trigger :GL == 1 and getlbc_gl == complete
          edit NAME 'gl12'
          edit KOPPLUNG '6'
          edit MEMBER '12'
          edit WALLT '03'
          edit NP '1'
          edit CLASS 'nf'
          label run ""
          label status ""
          label error "ok"
      endfamily
    endfamily
    family obs
      edit ASSIM '0'
      task getobs
        trigger :ASSIM == 1
        edit NP '1'
        edit CLASS 'ns'
        edit WALLT '01'
        edit NAME 'getobs'
        label run ""
        label status ""
        label error "ok"
      task bator
        trigger :ASSIM == 1 and getobs == complete
        edit NP '1'
        edit CLASS 'ns'
        edit WALLT '01'
        edit NAME 'bator'
        label run ""
        label status ""
        label error "ok"
      task bator3D
        trigger :ASSIM == 1 and getobs == complete
        edit NP '1'
        edit CLASS 'ns'
        edit WALLT '01'
        edit NAME 'bator3D'
        label run ""
        label status ""
        label error "ok"
    endfamily
    family main
      edit ASSIM '0'
      edit GL '0'
      family MEM_12
        task 927
          trigger ../../lbc/MEM_12/gl == complete or ../../lbc/MEM_12/901 == complete
          edit NAME '92712'
          edit ASSIM '0'
          edit KOPPLUNG '6'
          edit MEMBER '12'
          edit WALLT '03'
          edit NP '16'
          edit CLASS 'np'
          label run ""
          label status ""
          label error "ok"
        task 927surf
          trigger ../../lbc/MEM_12/gl == complete or ../../lbc/MEM_12/901 == complete
          edit NP '1'
          edit CLASS 'np'
          edit MEMBER '12'
          edit WALLT '01'
          edit NAME '927surf12'
          label run ""
          label status ""
          label error "ok"
        task sstex
          trigger :ASSIM == 1 and 927 == complete
          edit ASSIMC '6'
          edit NAME 'sstex12'
          edit MEMBER '12'
          edit WALLT '01'
          edit NP '1'
          edit CLASS 'ns'
          label run ""
          label status ""
          label error "ok"
        task addsurf
          trigger :ASSIM == 1 and sstex == complete
          edit ASSIMC '6'
          edit NAME 'addsurf12'
          edit MEMBER '12'
          edit WALLT '01'
          edit NP '1'
          edit CLASS 'ns'
          label run ""
          label status ""
          label error "ok"
        task screen
          trigger :ASSIM == 1 and addsurf == complete and ../../obs/bator3D == complete
          edit ASSIMC '6'
          edit EDA '1'
          edit NAME 'screen12'
          edit MEMBER '12'
          edit WALLT '03'
          edit NP '12'
          edit CLASS 'np'
          label run ""
          label status ""
          label error "ok"
        task screensurf
          trigger :ASSIM == 1 and addsurf == complete and ../../obs/bator == complete
          edit ASSIMC '6'
          edit NAME 'screensurf12'
          edit MEMBER '12'
          edit WALLT '03'
          edit NP '1'
          edit CLASS 'np'
          label run ""
          label status ""
          label error "ok"
        task canari
          complete :ASSIM == 0
          trigger :ASSIM == 1 and screensurf == complete
          edit ASSIMC '6'
          edit NAME 'canari12'
          edit MEMBER '12'
          edit SEDA '1'
          edit NP '1'
          edit CLASS 'np'
          edit WALLT '03'
          label run ""
          label status ""
          label error "ok"
        task minim
          complete :ASSIM == 0
          trigger :ASSIM == 1 and screen == complete
          edit ENSJK '0'
          edit ASSIMC '6'
          edit NAME 'minim12'
          edit MEMBER '12'
          edit WALLT '03'
          edit NP '12'
          edit CLASS 'np'
          label run ""
          label status ""
          label error "ok"
        task 001
          trigger 927 == complete and 927surf == complete and minim == complete and canari == complete
          edit ASSIMC '6'
          edit NAME '001_12'
          edit ASSIM '0'
          edit KOPPLUNG '6'
          edit MEMBER '12'
          edit WALLT '06'
          edit NP '480'
          edit STOCH '1'
          edit CLASS 'np'
          label run ""
          label status ""
          label error "ok"
        task progrid
          trigger 001  == complete
          edit NP '1'
          edit CLASS 'np'
          edit MEMBER '12'
          edit WALLT '01'
          edit NAME 'progrid12'
          label run ""
          label status ""
          label error "ok"
      endfamily
    endfamily
  endfamily
endsuite
