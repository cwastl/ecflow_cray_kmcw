          edit CLASS 'np'
          label run ""
          label info ""
          label error ""
        task screensurf
          complete :ASSIM == 1 and ../../obs/getobs:obsprog == 0 or :ASSIM == 0
          trigger :ASSIM == 1 and addsurf == complete and ../../obs/bator == complete
          edit NP '1'
          edit MEMBER '01'
          edit NAME 'screensurf01'
          edit CLASS 'np'
          label run ""
          label info ""
          label error ""
        task canari
