          edit NAME '001_01'
          edit MEMBER '01'
          edit STEPS15 '0'
          edit NP '360'
          edit STOCH '1'
          edit CLASS 'np'
          label run ""
          label info ""
          label error ""
          event e
        task progrid
          complete :LEAD < :LEADT
          trigger ../MEM_01/001:e
          edit NP '1'
          edit MEMBER '01'
          edit STEPS15 '0'
          edit NAME 'progrid01'
          edit CLASS 'np'
          label run ""
          label info ""
          event f
        task addgrib
          complete :LEAD < :LEADT
          trigger ../MEM_01/progrid:f
          edit NP '1'
          edit MEMBER '01'
          edit STEPS15 '0'
          edit NAME 'addgrib01'
          edit CLASS 'np'
          label run ""
          label info ""
          label error ""
      endfamily
    endfamily
  endfamily
endsuite
