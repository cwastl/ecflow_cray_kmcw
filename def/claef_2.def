        edit CLASS 'ns'
        label run ""
        label info ""
        label error ""
        event a
      family MEM_01
        task divlbc
          complete :L903 == 1 or :MEMBER == 00
          trigger :L903 == 0 and ../getlbc:a
          edit MEMBER '01'
          edit NP '1'
          edit NAME 'divlbc01'
          edit CLASS 'ns'
          label run ""
          label info ""
          label error ""
          event b
        task 901
          complete :L903 == 1 or :MEMBER == 00
          trigger :L903 == 0 and divlbc:b
          edit MEMBER '01'
          edit NP '1'
          edit NAME '901_01'
          edit CLASS 'ns'
          label run ""
          label info ""
          label error ""
          event c
        task getmars
          complete :L903 == 0
          trigger :L903 == 1
          edit NP '1'
          edit MEMBER '01'
          edit NAME 'getmars01'
          edit CLASS 'ns'
          label run ""
          label info ""
        task 903
          complete :L903 == 0
          trigger :L903 == 1 and getmars == complete
          edit ANZMEMB '1'
          edit NAME '903_01'
          edit KOPPLUNG '3'
          edit MEMBER '01'
          edit NP '16'
          edit CLASS 'np'
          label run ""
          label info ""
          label error ""
        task 903surf
          complete :L903 == 0
