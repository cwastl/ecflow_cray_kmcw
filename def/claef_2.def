  edit ECF_MICRO '%'
  edit ECF_LOGHOST '%SCHOST%-log'
  edit ACCOUNT 'atlaef'
  edit KOPPLUNG '3'
  edit ECF_HOME '/home/ms/at/kmcw/ecf'
  edit DATUM '20190804'
  edit USER 'kmcw'
  edit ECF_LOGPORT '36652'
  edit ECF_OUT '/scratch/ms/at/kmcw/ECF'
  edit CNF_DEBUG '0'
  edit SCHOST 'cca'
  family RUN_06
    edit VORHI '6'
    edit LEADCTL '6'
    edit LAUF '06'
