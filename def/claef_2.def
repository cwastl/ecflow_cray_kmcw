      task getobs
        complete :ASSIM == 0
        trigger :ASSIM == 1
        edit NP '1'
        edit NAME 'getobs'
        edit CLASS 'ns'
        label run ""
        label info ""
        meter obsprog -1 3 3
      task bator
        complete :ASSIM == 1 and getobs:obsprog == 0 or :ASSIM == 0
        trigger :ASSIM == 1 and getobs == complete
        edit NP '1'
        edit NAME 'bator'
        edit CLASS 'ns'
        label run ""
        label info ""
        label error ""
      task bator3D
        complete :ASSIM == 1 and getobs:obsprog == 0 or :ASSIM == 0
        trigger :ASSIM == 1 and getobs == complete
        edit NP '1'
        edit NAME 'bator3D'
        edit CLASS 'ns'
        label run ""
        label info ""
    endfamily
    family main
      edit ASSIM '0'
      edit L903 '1'
      edit GL '0'
      edit LEADT '6'
      family MEM_01
        task 927
          complete :L903 == 1
          trigger ../../lbc/MEM_01/901 == complete or ../../lbc/MEM_01/901:c
          edit NP '16'
          edit MEMBER '01'
          edit NAME '927_01'
          edit CLASS 'np'
          label run ""
          label info ""
          event d
        task 927surf
          complete :L903 == 1
          trigger ../../lbc/MEM_01/901 == complete or ../../lbc/MEM_01/901:c
          edit NP '1'
          edit MEMBER '01'
          edit NAME '927surf01'
          edit CLASS 'np'
          label run ""
          label info ""
          label error ""
        task sstex
          complete :ASSIM == 1 and ../../obs/getobs:obsprog == 0 or :ASSIM == 0
