# 4.11.1
suite claef_2
  edit ECF_EXTN '.ecf'
  edit ECF_TRIES '1'
  edit ACCOUNT 'atlaef'
  edit ECF_INCLUDE '/home/ms/at/kmcw/ecf/include'
  edit ECF_JOB_CMD '/usr/local/apps/schedule/1.4/bin/schedule kmcw cca %ECF_JOB% %ECF_JOBOUT%'
  edit ECF_MICRO '%'
  edit ECF_LOGHOST 'cca'
  edit DATUM '20190424'
  edit ECF_HOME '/home/ms/at/kmcw/ecf'
  edit ECF_LOGPORT '36652'
  edit ECF_OUT '/scratch/ms/at/kmcw/ECF'
  edit CNF_DEBUG '0'
  family RUN_06
    edit VORHI '6'
    edit LEADCTL '6'
    edit LAUF '06'
    edit LEAD '6'
    family lbc
      edit GL '0'
      family MEM_00
        task getlbc
          complete :GL == 1 or :MEMBER == 00
          trigger :GL == 0
          edit NAME 'getlbc00'
          edit SUITENAME 'claef_2'
          edit KOPPLUNG '3'
          edit MEMBER '00'
          edit USER 'kmcw'
          edit NP '1'
          edit CLASS 'ns'
          edit WALLT '02'
          label run ""
          label info ""
          label error ""
        task 901
          complete :GL == 1 or :MEMBER == 00
          trigger :GL == 0 and getlbc == complete
          edit NAME '901_00'
          edit KOPPLUNG '3'
          edit MEMBER '00'
          edit USER 'kmcw'
          edit NP '1'
          edit CLASS 'ns'
          edit WALLT '03'
          label run ""
          label info ""
          label error ""
        task getlbc_gl
          complete :GL == 0
          trigger :GL == 1
          edit NAME 'getlbcgl00'
          edit SUITENAME 'claef_2'
          edit KOPPLUNG '3'
          edit MEMBER '00'
          edit USER 'kmcw'
          edit NP '1'
          edit CLASS 'ns'
          edit WALLT '01'
          label run ""
          label info ""
        task gl
          complete :GL == 0
          trigger :GL == 1 and getlbc_gl == complete
          edit NAME 'gl00'
          edit KOPPLUNG '3'
          edit MEMBER '00'
          edit USER 'kmcw'
          edit NP '1'
          edit CLASS 'nf'
          edit WALLT '03'
          label run ""
          label info ""
      endfamily
    endfamily
    family obs
      edit ASSIM '0'
      task getobs
        complete :ASSIM == 0
        trigger :ASSIM == 1
        edit NP '1'
        edit CLASS 'ns'
        edit NAME 'getobs'
        edit USER 'kmcw'
        edit WALLT '01'
        label run ""
        label info ""
        meter obsprog -1 3 3
      task bator
        complete :ASSIM == 1 and getobs:obsprog == 0 or :ASSIM == 0
        trigger :ASSIM == 1 and getobs == complete
        edit NP '1'
        edit CLASS 'ns'
        edit NAME 'bator'
        edit USER 'kmcw'
        edit WALLT '01'
        label run ""
        label info ""
        label error ""
      task bator3D
        complete :ASSIM == 1 and getobs:obsprog == 0 or :ASSIM == 0
        trigger :ASSIM == 1 and getobs == complete
        edit NP '1'
        edit CLASS 'ns'
        edit NAME 'bator3D'
        edit USER 'kmcw'
        edit WALLT '01'
        label run ""
        label info ""
    endfamily
    family main
      edit ASSIM '0'
      edit GL '0'
      edit LEADT '6'
      family MEM_00
        task 927
          trigger :GL == 1 and ../../lbc/MEM_00/gl == complete or :GL == 0 and ../../lbc/MEM_00/901 == complete
          edit NAME '927_00'
          edit KOPPLUNG '3'
          edit MEMBER '00'
          edit USER 'kmcw'
          edit NP '16'
          edit CLASS 'np'
          edit WALLT '03'
          label run ""
          label info ""
        task 927surf
          trigger :GL == 1 and ../../lbc/MEM_00/gl == complete or :GL == 0 and ../../lbc/MEM_00/901 == complete
          edit NAME '927surf00'
          edit MEMBER '00'
          edit USER 'kmcw'
          edit NP '1'
          edit CLASS 'np'
          edit WALLT '01'
          label run ""
          label info ""
        task sstex
          complete :ASSIM == 1 and ../../obs/getobs:obsprog == 0 or :ASSIM == 0
          trigger :ASSIM == 1 and 927 == complete
          edit ASSIMC '6'
          edit NAME 'sstex00'
          edit MEMBER '00'
          edit USER 'kmcw'
          edit NP '1'
          edit CLASS 'ns'
          edit WALLT '01'
          label run ""
          label info ""
        task addsurf
          complete :ASSIM == 1 and ../../obs/getobs:obsprog == 0 or :ASSIM == 0
          trigger :ASSIM == 1 and sstex == complete
          edit ASSIMC '6'
          edit NAME 'addsurf00'
          edit MEMBER '00'
          edit USER 'kmcw'
          edit NP '1'
          edit CLASS 'ns'
          edit WALLT '01'
          label run ""
          label info ""
        task screen
          complete :ASSIM == 1 and ../../obs/getobs:obsprog == 0 or :ASSIM == 0
          trigger :ASSIM == 1 and addsurf == complete and ../../obs/bator3D == complete
          edit ASSIMC '6'
          edit EDA '1'
          edit NAME 'screen00'
          edit MEMBER '00'
          edit USER 'kmcw'
          edit NP '12'
          edit CLASS 'np'
          edit WALLT '03'
          label run ""
          label info ""
          label error ""
        task screensurf
          complete :ASSIM == 1 and ../../obs/getobs:obsprog == 0 or :ASSIM == 0
          trigger :ASSIM == 1 and addsurf == complete and ../../obs/bator == complete
          edit ASSIMC '6'
          edit NAME 'screensurf00'
          edit MEMBER '00'
          edit USER 'kmcw'
          edit NP '1'
          edit CLASS 'np'
          edit WALLT '03'
          label run ""
          label info ""
          label error ""
        task canari
          complete :ASSIM == 1 and ../../obs/getobs:obsprog == 0 or :ASSIM == 0
          trigger :ASSIM == 1 and screensurf == complete
          edit ASSIMC '6'
          edit NAME 'canari00'
          edit WALLT '03'
          edit MEMBER '00'
          edit SEDA '1'
          edit NP '1'
          edit CLASS 'np'
          edit USER 'kmcw'
          label run ""
          label info ""
        task minim
          complete :ASSIM == 1 and ../../obs/getobs:obsprog == 0 or :ASSIM == 0
          trigger :ASSIM == 1 and screen == complete
          edit ENSJK '1'
          edit ASSIMC '6'
          edit NAME 'minim00'
          edit MEMBER '00'
          edit USER 'kmcw'
          edit NP '12'
          edit CLASS 'np'
          edit WALLT '03'
          label run ""
          label info ""
        task 001
          trigger 927 == complete and 927surf == complete and minim == complete and canari == complete
          edit ASSIMC '6'
          edit NAME '001_00'
          edit KOPPLUNG '3'
          edit MEMBER '00'
          edit STEPS15 '0'
          edit USER 'kmcw'
          edit NP '396'
          edit STOCH '0'
          edit CLASS 'np'
          edit WALLT '06'
          label run ""
          label info ""
          label error ""
        task progrid
          complete :LEAD < :LEADT
          trigger 001  == complete
          edit NAME 'progrid00'
          edit MEMBER '00'
          edit STEPS15 '0'
          edit USER 'kmcw'
          edit NP '1'
          edit CLASS 'np'
          edit WALLT '01'
          label run ""
          label info ""
        task addgrib
          complete :LEAD < :LEADT
          trigger progrid  == complete
          edit NAME 'addgrib00'
          edit MEMBER '00'
          edit STEPS15 '0'
          edit USER 'kmcw'
          edit NP '1'
          edit CLASS 'np'
          edit WALLT '01'
          label run ""
          label info ""
          label error ""
      endfamily
    endfamily
  endfamily
endsuite
